// Placez vos paramètres dans ce fichier pour remplacer les paramètres par défaut
{
    "editor.fontSize": 14,
    "editor.wordWrap": "on",
    "latex-workshop.latex.toolchain": [{
        "command": "/path/to/pdflatex",
        "args": [
            "-synctex=1",
            "-interaction=nonstopmode",
            "-file-line-error",
            "%DOC%"
        ]
    }, {
        "command": "/path/to/bibtex",
        "args": [
            "%DOCFILE%"
        ]
    }, {
        "command": "/path/to/pdflatex",
        "args": [
            "-synctex=1",
            "-interaction=nonstopmode",
            "-file-line-error",
            "%DOC%"
        ]
    }, {
        "command": "/path/to/pdflatex",
        "args": [
            "-synctex=1",
            "-interaction=nonstopmode",
            "-file-line-error",
            "%DOC%"
        ]
    }],
    "latex-workshop.latex.clean.fileTypes": [
        "*.aux",
        "*.bbl",
        "*.blg",
        "*.idx",
        "*.ind",
        "*.lof",
        "*.lot",
        "*.out",
        "*.toc",
        "*.acn",
        "*.acr",
        "*.alg",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.fls",
        "*.log",
        "*.fdb_latexmk",
        "*.ptc",
        "*.synctex.gz",
        "*.run.xml",
        "*.bcf",
        "*.ilg"
    ],
    "latex-workshop.latex.clean.enabled": true,
    "latex-workshop.version": "2.10.0",
    "window.zoomLevel": 0
}